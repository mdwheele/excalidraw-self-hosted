{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Storage Service",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceFolder}/storage-service/www.js",
      "restart": true,
      "env": {
          "debug": "app:*",
      },
      "skipFiles": [
        "${workspaceFolder}/<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ]
    }
  ]
}
